{
  "name": "express-template",
  "version": "1.0.0",
  "description": "Express template",
  "main": "src/index.js",
  "repository": "https://github.com/arcticmatt/express-template.git",
  "author": "Matt Lim <mlimcollege@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "prisma migrate deploy && prisma generate && tsc",
    "createAuctionHouse": "ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/createAuctionHouse.ts",
    "eslint": "eslint 'src/**/*.ts'",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:gen": "prisma generate",
    "nodemon": "NODE_PATH=./dist nodemon dist/src/index.js",
    "run-server": "NODE_PATH=./dist node dist/src/index.js",
    "start": "ts-node-dev --ignore-watch 'src/generated/*.*' --require dotenv/config  --require tsconfig-paths/register --files src/index.ts",
    "tsc": "tsc"
  },
  "dependencies": {
    "@hammyflip/flipper-sdk": "^0.0.15",
    "@prisma/client": "^4.5.0",
    "@project-serum/anchor": "^0.25.0",
    "@solana/web3.js": "^1.66.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "invariant": "^2.2.4",
    "node-fetch": "2.6.6",
    "prisma": "^4.5.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/invariant": "^2.2.34",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "dotenv-run-script": "^0.2.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "yargs": "^17.6.0"
  }
}
